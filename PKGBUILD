pkgname=mariadb-fake
pkgver=1
pkgrel=1
pkgdesc="A fake package to fix dependancies on a missing mariadb/mysqldb installation"
arch=('any')
url="https://aur.archlinux.org/packages/ma/mariadb-fake"
license=(GPL)
provides=('mariadb')

build() { 
	echo 'Faking package mariadb'
}

package() {
	echo 'Building fake package'
}

